#results.copy
  ul.subnav
    case season
      when "2013"
        li
          h2 Auswertung aktuelle Saison
        li
          a(href='/auswertung/2012') Auswertung 2012/2013
        li
          a(href='/auswertung/2010') Auswertung 2010/2011 (Training)
      when "2012"
        li
          a(href='/auswertung/2013') Auswertung aktuelle Saison
        li
          h2 Auswertung 2012/2013
        li
          a(href='/auswertung/2010') Auswertung 2010/2011 (Training)
      default
        li
          a(href='/auswertung/2013') Auswertung aktuelle Saison
        li
          a(href='/auswertung/2012') Auswertung 2012/2013
        li
          h2 Auswertung 2010/2011 (Training)

  ul.subnav
    li
      h3 Ergebnisse #{group}. Spieltag #{season}/#{parseInt(season) + 1}
    li
      a(href='/api/points/' + season) JSON

  #guessesByGroup(data-season=season, data-group=group)
    .options
      select#guessesByGroupGroup.group
        - for (i=1; i<=34; i++)
          option(value='/auswertung/'+season+'/'+i, selected=(i==parseInt(group)))= i + ". Spieltag"
    .table
      p.loading
        img(src='/images/loading.gif')

  ul.subnav
    li
      h3 Saison Ãœbersicht #{season}/#{parseInt(season) + 1}
    li
      a.seasonChart.current(href='#') Chart
    li
      a.seasonTable(href='#') Tabelle
    li
      a(href='/api/points/2011') JSON

  #pointsBySeasonTable(data-season=season, data-group=group)
    .table
      p.loading
        img(src='/images/loading.gif')

  #pointsBySeasonChart(data-season=season, data-group=group)
